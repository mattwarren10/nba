<h1>Teams#index</h1>
<p>Find me in app/views/teams/index.html.erb</p>
<% @teams.each do |team| %>	
	<p>City: <%= team.city %></p>
	<p>Team Name: <%= team.name %></p>
	<p>Abbreviation: <%= team.abbreviation %></p>
	<p>NBA ID: <%= team.nba_com %></p>
	<p><%= image_tag "nba_team_logos/#{team.abbreviation}", class: 'team-logos' %></p>

<% end %>